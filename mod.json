{
	"geode": "5.0.0-beta.1",
	"version": "v1.3.2",
	"gd": {
        "win": "2.2081",
        "android": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
    },
	"id": "cvolton.compact_lists",
	"name": "Compact Lists",
	"developer": "Cvolton",
	"description": "Makes level cells smaller",
	"links": {
		"source": "https://github.com/Cvolton/compactlists-geode"
	},
    "dependencies": {
        "geode.node-ids": ">=v1.22.0"
    },
	"settings": {
        "enable-compact-lists": {
            "name": "Enable compact lists",
            "description": "Whether the mod should do anything at all",
            "type": "bool",
            "default": true
        },
        "show-toggle": {
            "name": "Show toggle in search",
            "description": "Whether the button to toggle compact lists should be shown in search",
            "type": "bool",
            "default": true
        }
	}
}
